{
  "name": "nachos-ui",
  "description": "Nachos UI is a React Native component library",
  "version": "0.1.2",
  "humanName": "Nachos UI",
  "license": "MIT",
  "main": "./lib/index.js",
  "author": {
    "name": "Petr Brzek",
    "email": "petr@avocode.com"
  },
  "repository": "avocode/nachos-ui",
  "keywords": ["react", "react-component", "react-native", "ui-kit"],
  "scripts": {
    "build": "babel src/ -d lib/",
    "build:web": "webpack --progress -p",
    "prepublish": "npm run build",
    "docs": "rsg \"docs/*.js\" -c \"styleguide.js\" -v",
    "docs:run": "http-server ./styleguide",
    "docs:prod": "npm run docs -- -r \"nachos-ui/docs\"",
    "start": "echo \"run `exp start` from native-example dir\"",
    "start:web":
      "webpack-dev-server --port 3000 --config webpack.config.js --inline --hot --colors --quiet",
    "lint": "eslint src docs",
    "lint:fix": "eslint --fix src docs",
    "fmt":
      "prettier --print-width 70 --write '{src,docs}/**/*.js'; npm run lint:fix; gulp semistrip",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/helpers/*.js"
    ],
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "globals": {
      "__DEV__": true
    },
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/enzyme_setup.js",
    "testMatch": ["<rootDir>/src/**/?(*.)spec.js"]
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-native": ">=0.52.0"
  },
  "dependencies": {
    "md5": "^2.2.1",
    "prop-types": "^15.6.1",
    "query-string": "^4.3.1",
    "react-art": "^16.3.2",
    "react-native-vector-icons": "^4.6.0"
  },
  "devDependencies": {
    "@expo/vector-icons": "^6.3.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.1.0",
    "estraverse-fb": "^1.3.1",
    "exp": "53.1.0",
    "expobook": "^0.0.4",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.5.4",
    "http-server": "^0.9.0",
    "jest": "^22.4.0",
    "jest-canvas-mock": "^1.0.2",
    "json-loader": "^0.5.4",
    "prettier": "^1.12.1",
    "raw-loader": "^0.5.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-native-mock": "^0.3.1",
    "react-native-web": "^0.6.1",
    "react-test-renderer": "^16.3.1",
    "rsg-alt": "^3.15.0",
    "string-replace-webpack-plugin": "^0.0.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
